{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "probableDateOfDelivery": {
      "type": "date"
    },
    "babyGender": {
      "type": "enumeration",
      "enum": [
        "Masculino",
        "Feminino"
      ]
    },
    "fatherName": {
      "type": "string"
    },
    "babyName": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "client"
    },
    "lists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::list.list",
      "mappedBy": "client"
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "client"
    },
    "acceptTerm": {
      "type": "boolean",
      "default": false
    },
    "acceptTermDate": {
      "type": "datetime"
    },
    "acceptPrivacyPoliciesDate": {
      "type": "datetime"
    },
    "acceptPrivacyPolicies": {
      "type": "boolean",
      "default": false
    },
    "childbirth_plans": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::childbirth-plan.childbirth-plan",
      "mappedBy": "clients"
    }
  }
}
